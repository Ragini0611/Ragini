import math
print (math.sin(math.pi/3))
print (math.cos(math.pi))
print (math.tan(math.pi/6))
print (math.sin(0.8660254037844386))
power = math.pow(5,8)
print ( power)
sqrt = math.sqrt(400)
print (sqrt)
e= 2.718
power = math.pow(5,e)
print(power)
print (math.log2(1024))
print (math.log10(1024))
print (math.floor(23.56))
print (math.ceil(23.56))